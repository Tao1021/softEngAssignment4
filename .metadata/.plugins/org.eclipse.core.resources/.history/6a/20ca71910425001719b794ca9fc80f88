dict1 = [{"Time":"06:00 - 09:00","avgVac":19.6141732283},{"Time":"09:00 - 12:00","avgVac":20.8898305085},{"Time":"12:00 - 15:00","avgVac":21.9537037037},{"Time":"15:00 - 18:00","avgVac":14.3267326733},{"Time":"18:00 - 21:00","avgVac":2.8888888889},{"Time":"21:00 - 00:00","avgVac":4.0149253731}]

for x in dict1:
    print(x)
    for y in dict1[x]:
        print(y, ": ", dict1[x][y])